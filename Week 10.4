n = int(input())
arr = list(map(int, input().split()))

num_swaps = 0
for i in range(n):
    for j in range(0, n-i-1):
        if arr[j] > arr[j+1]:
            arr[j], arr[j+1] = arr[j+1], arr[j]
            num_swaps += 1

print("List is sorted in {} swaps.".format(num_swaps))
print("First Element:", arr[0])
print("Last Element:", arr[-1])
